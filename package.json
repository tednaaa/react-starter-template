{
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start": "webpack serve --mode=development",
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "lint": "eslint ./src && eslint ./cypress",
    "format": "prettier . --write --ignore-path .gitignore",
